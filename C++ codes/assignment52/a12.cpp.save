#include <iostream>
#include <stdlib.h>
using namespace std;


//////// START OF MARKER FOR fib
int value;
int fib(int n) {
    if (n < 2)
        return n
    else 
        value = fib(n-2) + fib(n-1)
        return value
}

//////// END OF MARKER 



//////// DO NOT MODIFY CODE BEYOND THIS LINE

int main(int argc, char* argv[]) {
    cout << (fib(atoi(argv[1]))) <<endl;
    return 0;
}
